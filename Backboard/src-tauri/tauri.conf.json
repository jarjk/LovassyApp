{
  "build": {
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "LovassyApp - Backboard",
  "mainBinaryName": "LovassyApp - Backboard",
  "version": "2.1.1",
  "identifier": "me.lovassyapp.backboard",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "decorations": false,
        "fullscreen": false,
        "resizable": true,
        "title": "LovassyApp - Backboard",
        "width": 800,
        "height": 600,
        "minWidth": 600,
        "minHeight": 400,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
